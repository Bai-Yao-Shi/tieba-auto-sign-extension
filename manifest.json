{
  "manifest_version": 3,
  "name": "贴吧自动签到",
  "version": "1.0",
  "description": "每天定时签到百度贴吧，并记录签到日志",
  "permissions": [
    "storage",
    "alarms",
    "cookies"
  ],
  "host_permissions": [
    "https://tieba.baidu.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
